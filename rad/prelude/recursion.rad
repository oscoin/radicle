;; y-combinators ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

(def Y
  (fn (h)
    ((fn [x] (x x))
     (fn (g)
       (h (fn (arg) ((g g) arg)))))))
(def Y2
  (fn (h)
    ((fn [x] (x x))
     (fn (g)
       (h (fn (arg1 arg2) ((g g) arg1 arg2)))))))

((fn ()
  (def fac (Y (fn (f) (fn [x] (if (< x 2) 1 (* x (f (- x 1))))))))
  (should-be "fac" (fac 10) 3628800)))
